{
  "name": "zhang13pro-blog",
  "private": true,
  "description": "bobo's blog, powered by VuePress 2, themed by Gungnir.",
  "license": "Apache-2.0",
  "author": "Lex <zhang13pro@gmail.com>",
  "scripts": {
    "dev": "vuepress-cli dev blog --clean-cache",
    "build": "rimraf dist blog/.vuepress/.temp && vuepress-cli build blog --clean-cache",
    "serve": "anywhere -h localhost -d blog/.vuepress/dist",
    "info": "vuepress-cli info"
  },
  "devDependencies": {
    "@vuepress/bundler-vite": "2.0.0-beta.45",
    "@vuepress/cli": "2.0.0-beta.45",
    "@vuepress/client": "2.0.0-beta.45",
    "anywhere": "^1.6.0",
    "rimraf": "^3.0.2",
    "vue": "3.2.33",
    "vuepress": "2.0.0-beta.45",
    "vuepress-theme-gungnir": "2.0.0-alpha.23"
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=7"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack"
      ]
    }
  }
}
